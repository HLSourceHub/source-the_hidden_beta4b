 	"$bluramount" "0.2"
	"$REFRACTTINT" "[1.0 0.7 0.7]"
	"$REFRACTTINTtexture" "vgui/hud/hdn_617vision"
	"$scale" "[1 1]"
	"$dudvmap" "dev/water_dudv"
 	"$normalmap" "dev/water_normal"
	"$dudvframe" "0"
	"$bumpframe" "0"
